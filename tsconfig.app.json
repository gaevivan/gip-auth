/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
        "@models/*": ["src/app/models/*"],
        "@components/*": ["src/app/components/*"]
    }
  },
  "include": [
      "src/**/*.ts"
  ],
  "exclude": [
      "**/*.prod.ts",
      "**/*.spec.ts"
  ]
}
